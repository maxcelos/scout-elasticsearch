{
    "name": "maxcelos/scout-elasticsearch",
    "description": "Add ElasticSearch support to Laravel Scout",
    "require-dev": {
        
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcelo Barros da Silva",
            "email": "maxcelos@outlook.com"
        }
    ],
    "require": {
        "laravel/scout": "^4.0",
        "elasticsearch/elasticsearch": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Maxcelos\\ScoutElasticSearch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maxcelos\\ScoutElasticSearch\\ElasticSearchProvider"
            ],
            "aliases": {
                "ScoutElasticSearch": "Maxcelos\\ScoutElasticSearch\\ScoutElasticSearchFacade"
            }
        }
    }
}
